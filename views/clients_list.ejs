<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <link rel='stylesheet' href="/stylesheets/style.css" />
    <script src="/javascripts/jquery-3.6.0.min.js"></script>
</head>
<!-- IF TEMPLATE ADDED, DELETE ABOVE -->

<body>
    <h1>
        <%= title %>
    </h1>
    <br>
    <br>
    <table border="1">
        <tr>
            <td>번호</td>
            <td>구분</td>
            <td>이름</td>
            <td>전화번호</td>
            <td>주소</td>
        </tr>
        <%
            for (var i=0; i<clients.length; i++){
                var oneItem = clients[i];
        %>
            <tr style="cursor:pointer;" onClick="location.href='/admin/clients_details/<%=oneItem.RID%>' " onMouseOver=" window.status = '<%=oneItem.RID%>' " onMouseOut=" window.status = '' ">
                <td>
                    <%=i+1%>
                </td>
                <td>
                    <%if(oneItem.Ucase==="0"){%>
                        구매자
                        <%}else{%>
                            판매자
                            <%}%>
                </td>
                <td>
                    <%=oneItem.Rname%>
                </td>
                <td>
                    <%=oneItem.Phone%>
                </td>
                <td>
                    <%=oneItem.Address%>
                </td>
            </tr>
            <%
        }
        %>
    </table>
</body>